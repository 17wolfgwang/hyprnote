{
  "$schema": "../gen/schemas/mobile-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "auth:default",
    "notification:default",
    "deep-link:default",
    "misc:default",
    "db:default",
    "template:default",
    "store:default",
    {
      "identifier": "http:default",
      "allow": [
        { "url": "http://localhost:*" },
        { "url": "http://127.0.0.1:*" },
        { "url": "https://app.hyprnote.com/*" }
      ]
    }
  ]
}
