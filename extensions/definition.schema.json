{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "ExtensionDefinition",
  "type": "object",
  "required": [
    "cloud_only",
    "default",
    "description",
    "id",
    "implemented",
    "plugins",
    "tags",
    "title"
  ],
  "properties": {
    "cloud_only": {
      "type": "boolean"
    },
    "default": {
      "type": "boolean"
    },
    "description": {
      "type": "string"
    },
    "id": {
      "type": "string"
    },
    "implemented": {
      "type": "boolean"
    },
    "plugins": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "tags": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "title": {
      "type": "string"
    }
  }
}
